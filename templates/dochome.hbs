<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Booked Appointments</title>
</head>
<body>
    <h1>Booked Appointments</h1>
    <div id="appointments-list">
        <table>
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Address</th>
                    <th>Phone Number</th>
                </tr>
            </thead>
            <tbody>
                {{#each patients}}
                <tr>
                    <td>{{this.FirstName}}</td>
                    <td>{{this.LastName}}</td>
                    <td>{{this.Address}}</td>
                    <td>{{this.PhoneNo}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>

    <!-- Include client-side JavaScript code -->
    <script src="/patients.js"></script>
</body>
</html>
